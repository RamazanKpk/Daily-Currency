@model IEnumerable<Entitiy.Concrate.DailyCurrency>

@{
    ViewBag.Title = "Currency";
}
<h2>Currency</h2>

<link rel="stylesheet"  href="~/Content/Style/Style.css"/>
<div id="container">
    <form id="filterForm">
        <label for="startDate">Start Date</label>
        <input type="datetime-local" id="startDate" name="StartDate" />
        &nbsp;&nbsp;&nbsp;&nbsp;
        <label for="endDate">End Date</label>
        <input type="datetime-local" id="endDate" name="EndDate" />
        &nbsp;&nbsp;&nbsp;&nbsp;
        <label for="codeCurrency">Currency Code</label>
        <input type="text" id="codeCurrency" name="CodeCurrency" placeholder="Currency Code" />
        &nbsp;&nbsp;&nbsp;&nbsp;

        <button type="button" id="filterCodeCurrency" class="btn btn-warning fw-medium">Filter Currencies</button>
    </form>
</div>

<div id="tableContainer">
    <table id="currencyTable" class="table  table-striped">

        <thead>
            <tr>
                <th>Date</th>
                <th>Currency Code</th>
                <th>Unit</th>
                <th>Currency Name</th>
                <th>Forex Buying</th>
                <th>Forex Selling</th>
                <th>Banknote Buying</th>
                <th>Banknote Selling</th>
            </tr>
        </thead>
        <tbody>
        </tbody>

    </table>
    <table id="currencySDRTable" class="table  table-striped">

        <thead>
            <tr>
                <th>Information</th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
        </tbody>

    </table>
</div>


<script type="text/javascript" src="~/Scripts/js/daily.js"></script>